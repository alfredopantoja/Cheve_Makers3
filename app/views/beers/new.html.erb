<%= form_for([@user, @beer]) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :beer_type %>
	<%= f.text_field :beer_type %>
	
	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>	
